<?xml version="1.0" encoding="UTF-8" ?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans.xsd
    http://www.springframework.org/schema/context
    http://www.springframework.org/schema/context/spring-context.xsd"
    default-init-method="defaultInit" default-destroy-method="defaultDestroy">

    <!-- 慕课网-Spring入门篇，aware接口 -->

    <bean id="moocApplicationContextAware" class="com.immoc.spring_intro.aware.MoocApplicationContextAware">
    </bean>

<!--    调用了MoocBeanNameAware的构造器-->
    <bean id="moocBeanNameAware" class="com.immoc.spring_intro.aware.MoocBeanNameAware">
    </bean>
</beans>